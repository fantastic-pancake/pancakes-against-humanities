{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome: Attach",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"url": "http://localhost:3000/*"
		},

		{
			"name": "Server: Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": true,
			"localRoot": "${workspaceRoot}",
			"outDir": "${workspaceRoot}/build"
		}
	]
}